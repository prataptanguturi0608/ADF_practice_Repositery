{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				},
				{
					"name": "input1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          emp_id as string,",
				"          emp_name as string,",
				"          age as string,",
				"          dep_id as string,",
				"          dep_name as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 aggregate(groupBy(emp_id,",
				"          emp_name,",
				"          age,",
				"          dep_id,",
				"          dep_name),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          emp_id,",
				"          emp_name,",
				"          age,",
				"          dep_id,",
				"          dep_name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}